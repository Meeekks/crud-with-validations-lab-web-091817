<h1>Song Index</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
      <td><%= link_to "View", song_path(song) %></td>
      <td><%= link_to "Edit", edit_song_path(song) %></td>
      <td><%= link_to "Delete", song, method: :destroy, data: { confirm: 'For real?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
